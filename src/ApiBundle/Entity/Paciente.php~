<?php

namespace ApiBundle\Entity;

/**
 * Paciente
 */
class Paciente
{

    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $dni;

    /**
     * @var string
     */
    private $data;

    /**
     * @var string
     */
    private $mail;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set dni
     *
     * @param integer $dni
     *
     * @return Paciente
     */
    public function setDni($dni)
    {
        $this->dni = $dni;

        return $this;
    }

    /**
     * Get dni
     *
     * @return integer
     */
    public function getDni()
    {
        return $this->dni;
    }

    /**
     * Set data
     *
     * @param string $data
     *
     * @return Paciente
     */
    public function setData($data)
    {
        $this->data = $data;

        return $this;
    }

    /**
     * Get data
     *
     * @return string
     */
    public function getData()
    {
        return $this->data;
    }

    /**
     * Set mail
     *
     * @param string $mail
     *
     * @return Paciente
     */
    public function setMail($mail)
    {
        $this->mail = $mail;

        return $this;
    }

    /**
     * Get mail
     *
     * @return string
     */
    public function getMail()
    {
        return $this->mail;
    }
}
